{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "test-StreamChannelFactory",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/debug/tests/TestStreamChannel.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [ { "name": "LD_LIBRARY_PATH", "value": "${workspaceFolder}/build/lib/debug:$LD_LIBRARY_PATH" } ],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "release-TestStreamChannelFactory",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/release/tests/TestStreamChannel.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [ { "name": "LD_LIBRARY_PATH", "value": "${workspaceFolder}/build/lib/release:$LD_LIBRARY_PATH" } ],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
